node{
   stage('SCM Checkout'){
     git 'https://github.com/kaustubh0228/sonarpipelineex'
   }
   stage('SonarQube analysis') {
    withSonarQubeEnv(credentialsId: '0825e465-44bc-4889-bb8e-3ff6fd2ec620', installationName: 'SonarQubeScanner') { // You can override the credential to be used
      sh 'mvn org.sonarsource.scanner.maven:sonar-maven-plugin:3.6.0.1398:sonar'
    }
  }
 }